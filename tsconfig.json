{
  "extends": "./.wxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$lib": ["./src/lib"],
      "$lib/*": ["./src/lib/*"]
    },
    "plugins": [{
      "name": "typescript-svelte-plugin",
      "enabled": true,
      "assumeIsSvelteProject": false
    }],
    "useDefineForClassFields": true
  }
}
